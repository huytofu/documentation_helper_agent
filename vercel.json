{
  "version": 2,
  "builds": [
    {
      "src": "agent/graph/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "agent/graph/app.py"
    }
  ],
  "env": {
    "USE_HUGGINGFACE": "true",
    "USE_INFERENCE_CLIENT": "false",
    "ENVIRONMENT": "production",
    "PROVISIONED_CONCURRENCY": "1",
    "CONCURRENCY_LIMIT": "5",
    "LOG_LEVEL": "INFO"
  }
} 